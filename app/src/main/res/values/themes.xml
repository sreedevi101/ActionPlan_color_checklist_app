<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Checklist" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color.
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
         Secondary brand color.
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
         Status bar color.
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
         Customize your theme here. -->

        <!--
            This is a dummy Theme extending from The Default Android Theme
            This will not be used in this tutorial but would be useful in defining the properties you would
            want as common in all the themes.
         -->
    </style>

    <style name="Theme.Checklist.Professional" parent="Theme.Checklist">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColorSet1</item>
        <item name="colorPrimaryVariant">@color/primaryDarkColorSet1</item>
        <item name="colorOnPrimary">@color/primaryTextColorSet1</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryColorSet1</item>
        <item name="colorSecondaryVariant">@color/secondaryLightColorSet1</item>
        <item name="colorOnSecondary">@color/secondaryTextColorSet1</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="android:textColorPrimary">@color/primaryColorSet1</item>
        <item name="android:textColorSecondary">@color/secondaryColorSet1</item>
        <item name="android:titleTextColor">@color/primaryTextColorSet1</item>
        <item name="android:actionMenuTextColor">@color/primaryTextColorSet1</item>

        <!-- THIS is where you can color the arrow and the overflow button! -->
        <item name="actionOverflowButtonStyle">@style/actionOverflowButtonStyle</item> <!-- sets the style for the overflow button -->

        <item name="android:editTextColor">@color/primaryDarkColorSet1</item>
        <item name="android:textColorTertiary">@color/secondaryColorSet1</item>
        <item name="android:textColorPrimaryInverse">@color/primaryDarkColorSet1</item>
        <item name="android:textColorSecondaryInverse">@color/secondaryColorSet1</item>
        <item name="android:textColorTertiaryInverse">@color/secondaryColorSet1</item>
    </style>

